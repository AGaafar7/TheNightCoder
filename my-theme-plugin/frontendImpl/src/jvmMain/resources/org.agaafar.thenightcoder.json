{
  "meta": {
    "theme.name": "The Night Coder",
    "theme.kind": "Dark",
    "version": 2
  },
  "textAttributes": {
    "editor.selection": {
      "backgroundColor": "#98FB98"
    },
    "editor.selection.focused": {
      "backgroundColor": "#f23857"
    },
    "editor.indentGuide": {
      "foregroundColor": "#CD5C5C",
      "textDecoration": {
        "style": "DASHED"
      }
    },
    "problem.error": {
      "textDecoration": {
        "color": "#8B0000",
        "style": "SOLID",
        "type": "UNDERLINE",
        "thickness": 2.0
      }
    }
  },
  "colors": {
    "tab.background.default": "#FFDEAD",
    "tab.background.hovered": "#F5DEB3",
    "tab.background.selected": "#d90024",
    "tab.indicator": "IndicatorColor",
    "popup.tab.background.default": "#FFE4C4",
    "popup.tab.background.hovered": "#D8BFD8",
    "popup.tab.background.selected": "#d90024",
    "popup.tab.indicator": "IndicatorColor",
    "editor.background": "#FAFAD2",
    "editor.caret.background": "#FF7F50",
    "editor.caret.border": "#8A2BE2",
    "tool.background": "#7FFFD4",
    "border": "#c4314a",
    "border.focused": "#d90024",
    "button.dangerous.background.default": "#e60505",
    "button.dangerous.background.disabled": "Transparent",
    "button.dangerous.background.hovered": "#f00c1f",
    "button.dangerous.background.pressed": "#cc0213",
    "button.dangerous.border.default": "#d90024",
    "button.dangerous.border.disabled" : "#2e2e2e",
    "button.dangerous.border.hovered" : "#f00c1f",
    "button.dangerous.border.pressed" : "#cc0213" ,
    "button.dangerous.focusBorder" : "#0a0a0a",
    "button.positive.background.default": "#e60505",
    "button.positive.background.disabled": "Transparent",
    "button.positive.background.hovered": "#f00c1f",
    "button.positive.background.pressed": "#cc0213",
    "button.positive.border.default": "#d90024",
    
  },
  "palette": {
    "IndicatorColor": "#DA70D6"
  }
}
